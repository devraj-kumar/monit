<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta content="width=device-width, initial-scale=1" name="viewport" />
  <meta name="description" content="Responsive Admin Template" />
  <meta name="author" content="RedstarHospital" />

  <!-- google font -->
  <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&amp;subset=all" rel="stylesheet"
    type="text/css" />
  <!-- icons -->
  <link href="fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
  <link rel="stylesheet" href="http://radixtouch.in/templates/admin/smart/source/assets/plugins/iconic/css/material-design-iconic-font.min.css">
  <!-- bootstrap -->
  <link href="http://radixtouch.in/templates/admin/smart/source/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <!-- style -->
  <link rel="stylesheet" href="http://radixtouch.in/templates/admin/smart/source/assets/css/pages/extra_pages.css">
  <!-- favicon -->
  <link rel="shortcut icon" href="http://radixtouch.in/templates/admin/smart/source/assets/img/favicon.ico" />
</head>

<body>
  <div class="limiter">
    <div class="container-login100 page-background">
      <div class="wrap-login100">
        <%= form_tag sessions_path do |form| %>
          <span class="login100-form-logo">
            <img alt="" src="http://radixtouch.in/templates/admin/smart/source/assets/img/logo-2.png">
          </span>
          <span class="login100-form-title p-b-34 p-t-27">
            Log in
          </span>

          <div align="center"  data-validate="Enter username">
            <%= label_tag :email %>
            <%=  text_field_tag :email %>





          </div>
          <br>
          <div align="center"  data-validate="Enter password">
            <%= label_tag :password %>
            <%= password_field_tag :password %>


          </div>
          <br>
          <br>

          <div class="container-login100-form-btn">
            <%= submit_tag "Login" %>
          </div>

          <div class="text-center p-t-30">
            <a class="txt1" href="forgot_password.html">
              Forgot Password?
            </a>
          </div>
         <% end %>
      </div>
    </div>
  </div>
  <!-- start js include path -->
  <script src="http://radixtouch.in/templates/admin/smart/source/assets/plugins/jquery/jquery.min.js"></script>
  <!-- bootstrap -->
  <script src="http://radixtouch.in/templates/admin/smart/source/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
  <script src="http://radixtouch.in/templates/admin/smart/source/assets/js/pages/extra-pages/pages.js"></script>
  <!-- end js include path -->
</body>


